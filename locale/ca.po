msgctxt "model:ir.message,text:msg_date_format_error"
msgid ""
"Error building domain of type Date.\n"
"Please, check the format:\n"
"Field: %(field)s\n"
"Value: %(value)s\n"
"Format: %%d/%%m/%%Y"
msgstr ""
"Error de domini pel tipus Date.\n"
"Revisi que és una data.\n"
"Camp: %(field)s\n"
"Valor: %(value)s\n"
"Format: %%d/%%m/%%Y"

msgctxt "model:ir.message,text:msg_datetime_format_error"
msgid ""
"Error building domain of type DateTime or Timestamp.\n"
"Please, check the format:\n"
"Field: %(field)s\n"
"Value: %(value)s\n"
"Format: %%d/%%m/%%Y %%H:%%M:%%S"
msgstr ""
"Error de domini pel tipus DateTime o Timestamp.\n"
"Revisi que el format.\n"
"Camp: %(field)s\n"
"Valor: %(value)s\n"
"Format: %%d/%%m/%%Y %%H:%%M:%%S"

