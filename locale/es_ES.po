msgctxt "error:carrier.print.shipment:"
msgid "The carrier \"%(carrier)s\" has not any API method available."
msgstr "El transportista \"%(carrier)s\" no tiene ninguna API disponible."

msgctxt "error:carrier.print.shipment:"
msgid ""
"The shipment \"%(shipment)s\" has not any carrier assigned. Please, select "
"one carrier before trying to print the label."
msgstr ""
"El envío \"%(shipment)s\" no tiene ningún transportista asignado. Por favor,"
" seleccione uno antes de intentar imprimir la etiqueta."

msgctxt "error:carrier.print.shipment:"
msgid "The shipment %(shipment)s is not in any of these states \"%(state)s\"."
msgstr "El envío %(shipment)s no está en ninguno de estos estados \"%(state)s\"."

msgctxt "error:carrier.print.shipment:"
msgid "The shipment (%(shipment)s) is already sent."
msgstr "El envío (%(shipment)s) ya está enviado."

msgctxt "error:carrier.print.shipment:"
msgid ""
"The zip \"%(zip)s\" of the shipment \"%(shipment)s\" is not available for "
"this carrier."
msgstr ""
"El código postal \"%(zip)s\" del envío \"%(shipment)s\" no está disponible "
"para este transpostista."

msgctxt "error:carrier.print.shipment:"
msgid ""
"You've selected shipments with different methods of shipping. Please, select"
" shipments of a unique carrier."
msgstr ""
"Ha seleccionado envíos con diferentes métodos. Por favor, seleccione envíos "
"de un único transportista."

msgctxt "error:carrier.send.shipments:"
msgid "Not available method API in carrier \"%(carrier)s\""
msgstr "No se encuentra el método API del transportista \"%(carrier)s\""

msgctxt "error:carrier.send.shipments:"
msgid "Select a carrier in shipment \"%(shipment)s\""
msgstr "Selecciona en los albaranes un transportista para enviar \"%(shipment)s\""

msgctxt "error:carrier.send.shipments:"
msgid "Shipment \"%(code)s\" not available to send zip \"%(zip)s\""
msgstr "El envío \"%(code)s\" no se puede enviar a este código postal \"%(zip)s\""

msgctxt "error:carrier.send.shipments:"
msgid ""
"Shipment %(code)s not have address details: street, zip, city or country."
msgstr ""
"El albarán %(code)s no dispone en la dirección: calle, código postal, ciudad"
" o país."

msgctxt "error:carrier.send.shipments:"
msgid "Shipment (%(shipment)s) was sended"
msgstr "El envío (%(shipment)s) ya se ha enviado anteriormente"

msgctxt "error:carrier.send.shipments:"
msgid "Shipment ID (%(shipment)s) not state \"%(state)s\""
msgstr ""
"El envío ID (%(shipment)s) no se encuentra en el estado del albarán "
"\"%(state)s\""

msgctxt "error:carrier.send.shipments:"
msgid ""
"Successfully:\n"
"%(references)s\n"
"\n"
"Errors:\n"
"%(errors)s"
msgstr ""
"Con éxito:\n"
"%(references)s\n"
"\n"
"Errors:\n"
"%(errors)s"

msgctxt "error:carrier.send.shipments:"
msgid ""
"You select different shipments to send %(methods)s. Select shipment grouped "
"by carrier"
msgstr ""
"Ha seleccionado diferentes tipos de envío %(methods)s. Selecciona los "
"albaranes por transportista."

