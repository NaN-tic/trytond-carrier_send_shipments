<tryton>
    <data grouped="1">
      <record model="ir.message" id="msg_domain_field_error">
          <field name="text">Error in field domain: %(domain)s</field>
      </record>
      <record model="ir.message" id="msg_domain_field_help">
          <field name="text">This field must returns a variable called "domain" with a Tryton domain.</field>
      </record>

      <record model="ir.message" id="msg_not_implemented_error">
          <field name="text">Error. Field of type %(field)s is not implemented yet.</field>
      </record>
      <record model="ir.message" id="msg_datetime_format_error">
          <field name="text">Error building domain of type DateTime or Timestamp.
Please, check the format:
Field: %(field)s
Value: %(value)s
Format: %%d/%%m/%%Y %%H:%%M:%%S</field>
      </record>
      <record model="ir.message" id="msg_date_format_error">
          <field name="text">Error building domain of type Date.
Please, check the format:
Field: %(field)s
Value: %(value)s
Format: %%d/%%m/%%Y</field>
      </record>
      <record model="ir.message" id="msg_number_format_error">
          <field name="text">Error building domain of type Float.
Please, ensure you have put a number.
Field: %(field)s
Value: %(value)s</field>
      </record>
      <record model="ir.message" id="msg_error_domain">
          <field name="text">Error "%s(domain)s".</field>
      </record>
      <record model="ir.message" id="msg_shipment_sended">
          <field name="text">Shipment "%(shipment)s" was sended. Can not delivery again.</field>
      </record>
      <record model="ir.message" id="msg_add_carrier">
          <field name="text">Select a carrier in shipment "%(shipment)s"</field>
      </record>
      <record model="ir.message" id="msg_not_carrier">
          <field name="text">Shipment "%(name)s" not have carrier</field>
      </record>
      <record model="ir.message" id="msg_not_carrier_api">
          <field name="text">Carrier "%(name)s" not have API</field>
      </record>
      <record model="ir.message" id="msg_shipment_zip">
          <field name="text">Not available "%(shipment)s" to delivery at zip: "%(zip)s"</field>
      </record>
      <record model="ir.message" id="msg_shipment_info">
          <field name="text">Successfully:
%(references)s

Errors:
%(errors)s</field>
      </record>
      <record model="ir.message" id="msg_shipment_not_tracking_ref">
          <field name="text">The is not a carrier tracking reference in shipment "%(shipment)s".</field>
      </record>
      <record model="ir.message" id="msg_several_shipments">
          <field name="text">Cannot print carrier labels for more than one shipment at once.</field>
      </record>
      <record model="ir.message" id="msg_shipmnet_delivery_address">
          <field name="text">Shipment "%(name)s" not have address details: street, zip, city or country.</field>
      </record>
      <record model="ir.message" id="msg_shipment_state">
          <field name="text">Shipment "%(shipment)s", state "%(state)s" is not available in "%(states)s" to send.</field>
      </record>
    </data>
</tryton>
